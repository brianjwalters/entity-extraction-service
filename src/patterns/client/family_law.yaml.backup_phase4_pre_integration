  quality_thresholds:
    min_confidence: 0.86
    precision_target: 0.93
    recall_target: 0.90

quality_metrics:
  precision_target: 0.93
  recall_target: 0.90
  f1_target: 0.91
  processing_speed: 400_documents_per_second

dependencies:
  requires:
    - legal_terminology_patterns
    - date_patterns
    - monetary_amount_patterns
  optional:
    - jurisdiction_specific_patterns
    - california_family_code_patterns
    - washington_rcw_patterns

testing:
  test_cases:
    - pattern: custody_terms.joint_legal_custody
      text: The parties shall have joint legal custody of the minor children
      expected_entities: 1
    - pattern: visitation_schedules.visitation_pattern
      text: visitation every other weekend from Friday to Sunday
      expected_entities: 1
    - pattern: financial_obligations.child_support_amount
      text: child support of $1,500 per month
      expected_entities: 1
    - pattern: parenting_plan_terms.decision_making_authority
      text: decision-making authority regarding education
      expected_entities: 1
    - pattern: best_interests_factors.stable_home_environment
      text: stable home environment for the children
      expected_entities: 1
    - pattern: child_identification.child_name_pattern
      text: minor children named Emma Johnson and Noah Johnson
      expected_entities: 1
    - pattern: family_law_standards.best_interests_standard
      text: in the best interests of the child
      expected_entities: 1
    - pattern: jurisdiction_concepts.home_state
      text: California is the child's home state
      expected_entities: 1
    - pattern: jurisdiction_concepts.emergency_jurisdiction
      text: emergency jurisdiction exercised due to imminent harm
      expected_entities: 1
    - pattern: procedural_documents.dissolution_petition
      text: petition for dissolution filed on March 15, 2024
      expected_entities: 1
    - pattern: procedural_documents.guardian_ad_litem_appointment
      text: GAL appointed to represent children's interests
      expected_entities: 1
    - pattern: property_division.community_property
      text: earnings during marriage are community property
      expected_entities: 1
    - pattern: child_protection.child_abuse_report
      text: CPS report filed regarding physical abuse
      expected_entities: 1
  performance_benchmarks:
